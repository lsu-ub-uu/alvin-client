{% extends "django_oai_pmh/base.xml" %}
{% load oai_pmh %}

{% block content %}
<ListRecords>   
   {% if metadataprefix == 'oai_dc' %}
        {{metadataoai_dc|safe }}
    {% endif %}
    {% autoescape on %}   
    {% if cursor < completeListSize %}
    <resumptionToken completeListSize="{{completeListSize}}" cursor="{{cursor}}">{{resumptionToken}}</resumptionToken>
    {% endif %}
    {% endautoescape %}
</ListRecords>
{% endblock %}
